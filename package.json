{
  "name": "erwinsmitblog",
  "private": true,
  "description": "Blog by Erwin Smit",
  "version": "1.0.0",
  "author": "Erwin Smit",
  "dependencies": {
    "gatsby": "^2.9.6",
    "gatsby-plugin-catch-links": "^2.0.15",
    "gatsby-plugin-feed": "^2.2.3",
    "gatsby-plugin-google-analytics": "^2.0.21",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.3",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.1.6",
    "gatsby-plugin-typography": "^2.2.13",
    "gatsby-remark-autolink-headers": "2.0.18",
    "gatsby-remark-copy-linked-files": "^2.0.13",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.0.16",
    "gatsby-remark-prismjs": "^3.2.11",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.40",
    "gatsby-transformer-remark": "^2.4.0",
    "gatsby-transformer-sharp": "^2.1.21",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "lodash": "^4.17.11",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "gatsby-cli": "^2.6.9",
    "gh-pages": "^2.0.1",
    "prettier": "^1.18.2"
  },
  "homepage": "https://erwinsmit.nl",
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --write '{gatsby-*.js,src/**/*.{js,jsx,json,css}}'",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "rm -rf public && rm -rf .cache && gatsby build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{gatsby-*.js,src/**/*.{js,jsx,json,css}}": [
      "yarn format",
      "git add"
    ]
  }
}
